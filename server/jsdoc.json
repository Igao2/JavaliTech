{
    "source": {
        "include": [
            "src"
        ],
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "./doc-cusnton-template/",
        "readme": "./README.md"
    }
}